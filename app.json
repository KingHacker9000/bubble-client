{
  "expo": {
    "name": "Bubble",
    "slug": "Bubble",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/bubbleSquare.png",
    "splash": {
      "image": "./assets/bubbleSquare.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "Bubble.bubbleapp"
    },
    "android": {
      "package": "Bubble.bubbleapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/bubbleSquare.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/bubbleSquare.png"
    }
  }
}
